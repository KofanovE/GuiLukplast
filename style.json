{
    "ShowLogs": true,
    "QMainWindow": [
        {
            "tittle": "Lukplast App"
        }
    ],
    "QSettings": [
        {
            "ThemeSettings": [
                {
                    "CustomTheme": [
                        {
                            "Background-color": "#1b1b27",
                            "Text-color": "#fff",
                            "Accent-color": "#00bfff",
                            "Theme-name": "Default-Theme",
                            "Icons-color": "#fff",
                            "Default-Theme": true

                        }

                        ]
                }
            ],
            "AppSettings": {
                "OrginizationName": "Lukplast App",
                "ApplicationName": "Lukplast App Company",
                "OrginizationDormain": "Lukplast App.org"
            }
        }
    ],
    "QPushButtonGroup": [
        {
		"Buttons": [
			"homeBtn",
			"accountBtn",
			"reportsBtn",
			"settingsBtn",
                        "helpBtn",
                        "aboutBtn"
		],
		"Style":[{
			"Active": "background-color: #1b1b27; border-left: 3px solid #00bfff; font-weight: bold;",
			"NotActive": "background-color: transparent; border: none;"
		}]
	}
    ],
    "QCustomSlideMenu": [
        {
		"name": "leftMenu",
		"defaultSize": [{
			"width": 0,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 200,
			"height": "parent"
		}],
		"toggleButton": [{
			"buttonName": "menuBtn",
			"icons": [{
				"whenMenuIsCollapsed": ":/icons/Icons/align-justify.png",
				"whenMenuIsExpanded": ":/icons/Icons/chevron-left.png"
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		
		}]
		
		
	},
        {
		"name": "rightMenu",
		"defaultSize": [{
			"width": 0,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 200,
			"height": "parent"
		}],
		"toggleButton": [{
			"buttonName": "pushButton"
			
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		
		}]
		
		
	}
    ],
    "QStackedWidget":[
		{
			"name":"mainPages",
			"transitionAnimation":[
				{
					"fade":[
						{
							"active": true,
							"duration":1000,
							"easingCurve": "OutBack"
						}
					],
					"slide":[
						{
							"active":true,
							"duration": 1000,
							"direction": "horizontal",
							"easingCurve": "OutBack"
						}
					]
				}
			],
			"navigation":[
				{
					"navigationButtons":[
						{
							"homeBtn": "homePage",
							"reportsBtn": "reportsPage",
                                                        "accountBtn": "accountsPage",
                                                        "settingsBtn": "settingsPage",
                                                        "helpBtn": "helpPage",
                                                        "aboutBtn": "aboutPage"
						}
					]
				}
			]
	
		}
	]
}
